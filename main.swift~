func swap(strings:inout[String],firstIndex:Int,secondIndex:Int) {
    let x = integers[firstIndex]
    let y = integers[secondIndex]
    integers.remove(at:firstIndex)
    integers.insert(y, at:firstIndex)
    integers.remove(at:secondIndex)
    integers.insert(x, at: secondIndex)
}
var integers:[String] = ["zebra" ,"cat", "dog", "monkey", "banana", "elephant", "apple", "boron"]


var last_position = integers.count - 1
var sap = true
while sap == true {
    sap = false
    for i in 0..<last_position {
        if integers[i] > integers[i + 1] {

//            swap(strings:&integers, firstIndex: (i), secondIndex: (i+1) )
            integers.swapAt(i, i+1)


            sap = true
        }


    }
  

}
print(integers)
